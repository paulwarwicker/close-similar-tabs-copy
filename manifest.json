{
  "manifest_version": 2,
  "name": "Close Similar Tabs Copy",
  "version": "0.0.1",
  "description": "Close tabs from same host",
  "homepage_url": "https://github.com/paulwarwicker/close-similar-tabs-copy",

  "icons": {
    "48": "tabs.svg",
	  "96": "tabs.svg"
  },

  "permissions": [
	  "activeTab",
	  "menus",
	  "tabs"
    ],

  "background": {
    "scripts": ["background.js"]
  }
}